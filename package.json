{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "electron-reload",
    "description": "#### basic api documentation for  [electron-reload (v1.1.0)](https://github.com/yan-foto/electron-reload#readme)  [![npm package](https://img.shields.io/npm/v/npmdoc-electron-reload.svg?style=flat-square)](https://www.npmjs.org/package/npmdoc-electron-reload) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-electron-reload.svg)](https://travis-ci.org/npmdoc/node-npmdoc-electron-reload)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "electron-reload": "*",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-electron-reload",
    "keywords": [
        "documentation",
        "electron-reload"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_electron_reload.js",
    "name": "npmdoc-electron-reload",
    "nameAlias": "npmdoc_electron_reload",
    "nameOriginal": "npmdoc-electron-reload",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-electron-reload"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "2017.4.27"
